{
  "name": "framework-guard",
  "version": "0.2.0",
  "description": "Toolkit for building middleware (Express-focused) with auth, errors, and responses.",
  "keywords": [
    "express",
    "middleware",
    "jwt",
    "auth",
    "cors",
    "error-handling"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --sourcemap --clean",
    "dev": "tsup src/index.ts --format cjs,esm --out-extension cjs=.cjs --out-extension esm=.mjs --dts --watch",
    "typecheck": "tsc --noEmit",
    "example": "tsx examples/express-basic.ts",
    "test": "tsx tests/core.test.ts",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "helmet": "^7.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.19.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.0"
  }
}
